{
    "name": "dvelum/db",
    "version": "4.0.1",
    "type": "library",
    "description": "DVelum 4.x DB extension",
    "keywords": ["dvelum","php","db"],
    "homepage": "https://github.com/dvelum/db",
    "license": "MIT",
    "authors": [
        {
            "name": "Kirill Yegorov",
            "role": "Founder, Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/dvelum/db/issues",
        "source": "https://github.com/dvelum/db"
    },
    "config": {
        "preferred-install": "dist",
        "classmap-authoritative": true
    },
    "require": {
        "php":">=7.4.0",
        "ext-mysqli" : "*",
        "laminas/laminas-db": "^2.11",
        "dvelum/cache":"^4.0",
        "psr/log": "^1.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^0.12",
        "codacy/coverage": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Dvelum\\Db\\": "src/"
        }
    }
}
